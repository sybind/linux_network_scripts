let g:netrw_banner = 0
let g:netrw_liststyle = 3
let g:netrw_browse_split = 4
let g:netrw_altv = 1
let g:netrw_winsize = 25
augroup ProjectDrawer
  autocmd!
  autocmd VimEnter * :Vexplore
augroup END

set hlsearch
set number
syntax on

filetype plugin indent on
set tabstop=2
set shiftwidth=2
set noexpandtab

set mouse=a

syntax on
